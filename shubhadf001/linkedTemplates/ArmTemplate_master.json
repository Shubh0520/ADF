{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shubhadf001"
		},
		"AzureDataLakeStorageADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageADLS'"
		},
		"AzureSqlDatabaseSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseSQL'"
		},
		"blob_landingdata_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_landingdata'"
		},
		"ls_adls_raw_layer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_raw_layer'"
		},
		"ls_adlsgen2_covidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsgen2_covidreportingdl'"
		},
		"ls_azureblob_covidreporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azureblob_covidreporting'"
		},
		"AzureDataLakeStorageADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeaccountra.dfs.core.windows.net/"
		},
		"ls_adls_raw_layer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeaccountra.dfs.core.windows.net/"
		},
		"ls_adlsgen2_covidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreptngdatalake01.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shubhadf001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageADLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorageADLS_accountKey')]"
					},
					"AzureSqlDatabaseSQL_connectionString": {
						"value": "[parameters('AzureSqlDatabaseSQL_connectionString')]"
					},
					"blob_landingdata_connectionString": {
						"value": "[parameters('blob_landingdata_connectionString')]"
					},
					"ls_adls_raw_layer_accountKey": {
						"value": "[parameters('ls_adls_raw_layer_accountKey')]"
					},
					"ls_adlsgen2_covidreportingdl_accountKey": {
						"value": "[parameters('ls_adlsgen2_covidreportingdl_accountKey')]"
					},
					"ls_azureblob_covidreporting_connectionString": {
						"value": "[parameters('ls_azureblob_covidreporting_connectionString')]"
					},
					"AzureDataLakeStorageADLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageADLS_properties_typeProperties_url')]"
					},
					"ls_adls_raw_layer_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_raw_layer_properties_typeProperties_url')]"
					},
					"ls_adlsgen2_covidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_covidreportingdl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}